{
  "name": "le-database-generator",
  "version": "1.0.0",
  "description": "Database generation system for Last Epoch game data - parses templates and web data to create structured database",
  "main": "scripts/build-database.js",
  "scripts": {
    "build": "node scripts/build-database.js",
    "build-force": "node scripts/build-database.js --force",
    "info": "node scripts/database-info.js",
    "generate-affix-templates": "node scripts/generate-affix-templates.js",
    "generate-set-templates": "node scripts/generate-set-templates.js",
    "generate-unique-templates": "node scripts/generate-unique-templates.js",
    "generate-subtype-templates": "node scripts/generate-subtype-templates.js",
    "validate": "node scripts/build-database.js --validate-only",
    "lint": "eslint scripts/"
  },
  "keywords": [
    "last-epoch",
    "database",
    "game-data",
    "xml-parsing",
    "html-parsing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^11.1.1",
    "jsdom": "^26.1.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^8.45.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}